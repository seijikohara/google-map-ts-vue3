{
  "name": "google-map-ts-vue3",
  "version": "1.2.1",
  "description": "Google Map Vue3 Component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seijikohara/google-map-ts-vue3.git"
  },
  "keywords": [
    "google",
    "maps",
    "vue3",
    "typescript"
  ],
  "author": "Seiji Kohara <sage.0118@gmail.com> (https://github.com/seijikohara)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seijikohara/google-map-ts-vue3/issues"
  },
  "homepage": "https://github.com/seijikohara/google-map-ts-vue3#readme",
  "type": "module",
  "main": "./dist/google-map-ts-vue3.umd.cjs",
  "module": "./dist/google-map-ts-vue3.js",
  "types": "./dist/src/components/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/components/index.d.ts",
        "default": "./dist/google-map-ts-vue3.js"
      },
      "require": {
        "types": "./dist/src/components/index.d.ts",
        "default": "./dist/google-map-ts-vue3.umd.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:check": "npx npm-check-updates",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@types/google.maps": "^3.58.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  }
}
